{
	"compilerOptions": {
		"allowJs": false,
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"pretty": true,
		"removeComments": false,
		"sourceMap": true,
		"strict": true,
		"target": "esnext",
		"jsx": "react"
	}
}
